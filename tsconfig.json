{
    "compilerOptions": {
        "module": "CommonJS",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strictFunctionTypes": true,
        "strict": true,
        "outDir": "./dist",
        "rootDir": "./src",
        "noImplicitAny": true,
        "skipLibCheck": true,
        "strictNullChecks": true,
        "target": "ES6"
    },
 
    "exclude": ["node_modules", "dist", "spec"]
}
